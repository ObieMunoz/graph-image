		"eslint": "^8.28.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.30.0",
		"prettier": "^3.0.3",
		"prettier-plugin-svelte": "^3.0.3",
		"publint": "^0.2.5",
